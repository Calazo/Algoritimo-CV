Algoritmo "conte0Ate"
Var
    Final, Contador: Inteiro
Inicio
    Contador <- 0
    Escreval("Deseja contar ate qual numero ?")
    Leia(Final)
    Enquanto (Contador < Final) faca
        Escreval(Contador)
        Contador <- Contador + 1
    FimEnquanto
    Escreval("Terminou de contar atÃ© ", Contador)

Fimalgoritmo
Algoritmo "triangulos"

Var
    L1, L2, L3: Real
    //Variaveis referentes aos lados do triangulo
Inicio
    Escreval("Primeiro lado : ")
    Leia(L1)
    Escreval("Segundo lado : ")
    Leia(L2)
    Escreval("Terceiro lado : ")
    Leia(L3)
    //Interação para recolher os valores referentes aos lados
    
Fimalgoritmo